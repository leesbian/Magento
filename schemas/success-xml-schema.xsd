<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:element name="magento_api">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="data_item" maxOccurs="unbounded">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="item_id" minOccurs="0" maxOccurs="1" type="xsd:int"/>
              <xsd:element name="product_id" minOccurs="0" maxOccurs="1" type="xsd:int"/>
              <xsd:element name="stock_id" minOccurs="0" maxOccurs="1" type="xsd:int"/>
              <xsd:element name="qty" minOccurs="0" maxOccurs="1" type="xsd:decimal"/>
              <xsd:element name="low_stock_date" minOccurs="0" maxOccurs="1" type="xsd:string"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
